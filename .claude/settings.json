      "Bash(go *)",
      "Bash(swift *)",
      "Bash(rm *)",
      "Bash(git reset *)",
      "Bash(git clean *)",
      "Bash(curl *)"
    ]
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "model": "opus",
  "sandbox": {
    "enabled": false,
    "autoAllowBashIfSandboxed": true,
    "allowUnsandboxedCommands": false,
    "network": {
      "allowLocalBinding": true,
      "allowUnixSockets": []
