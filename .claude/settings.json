      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)"
    ],
    "ask": [
      "Bash(git push *)",
      "Bash(gh *)",
      "Bash(npm install *)",
      "Bash(npm ci *)",
      "Bash(pip install *)",
      "Bash(pip3 *)",
      "Bash(brew *)",
